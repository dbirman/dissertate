@ARTICLE{Shanechi2017-iw,
  title    = "Rapid control and feedback rates enhance neuroprosthetic control",
  author   = "Shanechi, Maryam M and Orsborn, Amy L and Moorman, Helene G and
              Gowda, Suraj and Dangi, Siddharth and Carmena, Jose M",
  abstract = "Brain-machine interfaces (BMI) create novel sensorimotor pathways
              for action. Much as the sensorimotor apparatus shapes natural
